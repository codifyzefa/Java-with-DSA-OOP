import java.util.Scanner;
public class basics {
    public static void main(String[] args) {
        int a;
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter The Value of A : ");
        a=sc.nextInt();
        String ans;
        ans=(a%2==0)?"even" : "odd";
        System.out.println(ans);
    }
}
